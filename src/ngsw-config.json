{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "**.css",
          "/assets/i18n/**",
          "/index.html",
          "/*.js",
          "/manifest.json"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/js/**"
        ]
      }
    },
    {
      "name": "assets-deferred",
      "installMode": "lazy",
      "updateMode": "lazy",
      "resources": {
        "files": [
          "/assets/fonts/**",
          "/assets/images/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-freshness",
      "urls": [
        "/api/**",
        "/lib/**",
        "/backend/**",
        "/rabbitmq/**",
        "/rabbitmq-mgmt/**",
        "https://polyfill.io/**",
        "https://sentry.io/**"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 1,
        "maxAge": "1u",
        "timeout": "1s"
      }
    }
  ]
}
