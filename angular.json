{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "frontend": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "aot": true,
            "extractCss": true,
            "outputPath": "dist/frontend/browser",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              "src/assets/fonts",
              "src/assets/js",
              "src/assets/i18n",
              "src/assets/images",
              "src/assets/jobs",
              "src/assets/static",
              "src/assets/serverEndpoint.json",
              "src/assets/imageDerivates.json",
              "src/assets/themeData.json",
              "src/assets/piwik/piwik.js",
              {
                "glob": "click-service-worker.js",
                "input": "src",
                "output": "/"
              },
              {
                "glob": "robots.txt",
                "input": "src/assets",
                "output": "/"
              },
              {
                "glob": "noscript.css",
                "input": "src/styles",
                "output": "/"
              }
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/styles/themes"
              ]
            },
            "fileReplacements": [],
            "styles": [
              "node_modules/@fortawesome/fontawesome-svg-core/styles.css",
              "node_modules/prism-themes/themes/prism-ghcolors.css",
              "src/styles/styles.scss",
              {
                "inject": false,
                "input": "src/styles/themes/theme-arsnova-dot-click-contrast.scss",
                "bundleName": "theme-arsnova-dot-click-contrast"
              },
              {
                "inject": false,
                "input": "src/styles/themes/theme-blackbeauty.scss",
                "bundleName": "theme-blackbeauty"
              },
              {
                "inject": false,
                "input": "src/styles/themes/theme-elegant.scss",
                "bundleName": "theme-elegant"
              },
              {
                "inject": false,
                "input": "src/styles/themes/theme-Material.scss",
                "bundleName": "theme-Material"
              },
              {
                "inject": false,
                "input": "src/styles/themes/theme-Material-hope.scss",
                "bundleName": "theme-Material-hope"
              },
              {
                "inject": false,
                "input": "src/styles/themes/theme-decent-blue.scss",
                "bundleName": "theme-decent-blue"
              },
              {
                "inject": false,
                "input": "src/styles/themes/theme-Material-blue.scss",
                "bundleName": "theme-Material-blue"
              },
              {
                "inject": false,
                "input": "src/styles/themes/theme-spiritual-purple.scss",
                "bundleName": "theme-spiritual-purple"
              },
              {
                "inject": false,
                "input": "src/styles/themes/theme-GreyBlue-Lime.scss",
                "bundleName": "theme-GreyBlue-Lime"
              },
              {
                "inject": false,
                "input": "src/styles/themes/theme-westermann-blue.scss",
                "bundleName": "theme-westermann-blue"
              },
              {
                "inject": false,
                "input": "src/styles/themes/theme-Material.scss",
                "bundleName": "theme-default"
              }
            ],
            "scripts": [
              "node_modules/marked/lib/marked.js",
              "node_modules/prismjs/prism.js",
              "node_modules/prismjs/components.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "optimization": {
                "scripts": true,
                "styles": false
              },
              "outputHashing": "all",
              "sourceMap": {
                "hidden": false,
                "styles": true,
                "scripts": true
              },
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": true,
              "commonChunk": true,
              "buildOptimizer": true,
              "serviceWorker": true,
              "ngswConfigPath": "src/ngsw-config.json",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "styles": [
                "node_modules/@fortawesome/fontawesome-svg-core/styles.css",
                "node_modules/prism-themes/themes/prism-ghcolors.css",
                "src/styles/styles.scss",
                {
                  "inject": false,
                  "input": "src/styles/themes/theme-arsnova-dot-click-contrast.scss",
                  "bundleName": "theme-arsnova-dot-click-contrast"
                },
                {
                  "inject": false,
                  "input": "src/styles/themes/theme-blackbeauty.scss",
                  "bundleName": "theme-blackbeauty"
                },
                {
                  "inject": false,
                  "input": "src/styles/themes/theme-elegant.scss",
                  "bundleName": "theme-elegant"
                },
                {
                  "inject": false,
                  "input": "src/styles/themes/theme-Material.scss",
                  "bundleName": "theme-Material"
                },
                {
                  "inject": false,
                  "input": "src/styles/themes/theme-Material-hope.scss",
                  "bundleName": "theme-Material-hope"
                },
                {
                  "inject": false,
                  "input": "src/styles/themes/theme-decent-blue.scss",
                  "bundleName": "theme-decent-blue"
                },
                {
                  "inject": false,
                  "input": "src/styles/themes/theme-Material-blue.scss",
                  "bundleName": "theme-Material-blue"
                },
                {
                  "inject": false,
                  "input": "src/styles/themes/theme-spiritual-purple.scss",
                  "bundleName": "theme-spiritual-purple"
                },
                {
                  "inject": false,
                  "input": "src/styles/themes/theme-GreyBlue-Lime.scss",
                  "bundleName": "theme-GreyBlue-Lime"
                },
                {
                  "inject": false,
                  "input": "src/styles/themes/theme-westermann-blue.scss",
                  "bundleName": "theme-westermann-blue"
                },
                {
                  "inject": false,
                  "input": "src/styles/themes/theme-Material.scss",
                  "bundleName": "theme-default"
                }
              ]
            },
            "thmStaging": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "optimization": {
                "scripts": true,
                "styles": false
              },
              "outputHashing": "all",
              "sourceMap": {
                "hidden": false,
                "styles": true,
                "scripts": true
              },
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": true,
              "commonChunk": true,
              "buildOptimizer": true,
              "serviceWorker": true,
              "ngswConfigPath": "src/ngsw-config.json",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.thm.staging.ts"
                }
              ],
              "styles": [
                "node_modules/@fortawesome/fontawesome-svg-core/styles.css",
                "node_modules/prism-themes/themes/prism-ghcolors.css",
                "src/styles/styles.scss",
                {
                  "inject": false,
                  "input": "src/styles/themes/theme-arsnova-dot-click-contrast.scss",
                  "bundleName": "theme-arsnova-dot-click-contrast"
                },
                {
                  "inject": false,
                  "input": "src/styles/themes/theme-blackbeauty.scss",
                  "bundleName": "theme-blackbeauty"
                },
                {
                  "inject": false,
                  "input": "src/styles/themes/theme-elegant.scss",
                  "bundleName": "theme-elegant"
                },
                {
                  "inject": false,
                  "input": "src/styles/themes/theme-Material.scss",
                  "bundleName": "theme-Material"
                },
                {
                  "inject": false,
                  "input": "src/styles/themes/theme-Material-hope.scss",
                  "bundleName": "theme-Material-hope"
                },
                {
                  "inject": false,
                  "input": "src/styles/themes/theme-decent-blue.scss",
                  "bundleName": "theme-decent-blue"
                },
                {
                  "inject": false,
                  "input": "src/styles/themes/theme-Material-blue.scss",
                  "bundleName": "theme-Material-blue"
                },
                {
                  "inject": false,
                  "input": "src/styles/themes/theme-spiritual-purple.scss",
                  "bundleName": "theme-spiritual-purple"
                },
                {
                  "inject": false,
                  "input": "src/styles/themes/theme-GreyBlue-Lime.scss",
                  "bundleName": "theme-GreyBlue-Lime"
                },
                {
                  "inject": false,
                  "input": "src/styles/themes/theme-westermann-blue.scss",
                  "bundleName": "theme-westermann-blue"
                },
                {
                  "inject": false,
                  "input": "src/styles/themes/theme-Material.scss",
                  "bundleName": "theme-default"
                }
              ]
            },
            "thmBeta": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "optimization": {
                "scripts": true,
                "styles": false
              },
              "outputHashing": "all",
              "sourceMap": {
                "hidden": false,
                "styles": true,
                "scripts": true
              },
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": true,
              "commonChunk": true,
              "buildOptimizer": true,
              "serviceWorker": true,
              "ngswConfigPath": "src/ngsw-config.json",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.thm.beta.ts"
                }
              ],
              "styles": [
                "node_modules/@fortawesome/fontawesome-svg-core/styles.css",
                "node_modules/prism-themes/themes/prism-ghcolors.css",
                "src/styles/styles.scss",
                {
                  "inject": false,
                  "input": "src/styles/themes/theme-blackbeauty.scss",
                  "bundleName": "theme-blackbeauty"
                },
                {
                  "inject": false,
                  "input": "src/styles/themes/theme-westermann-blue.scss",
                  "bundleName": "theme-westermann-blue"
                },
                {
                  "inject": false,
                  "input": "src/styles/themes/theme-westermann-blue.scss",
                  "bundleName": "theme-default"
                }
              ]
            },
            "westermann": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "optimization": {
                "scripts": true,
                "styles": false
              },
              "outputHashing": "all",
              "sourceMap": {
                "hidden": false,
                "styles": true,
                "scripts": true
              },
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": true,
              "commonChunk": true,
              "buildOptimizer": true,
              "serviceWorker": true,
              "ngswConfigPath": "src/ngsw-config.json",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.westermann.ts"
                }
              ],
              "styles": [
                "node_modules/@fortawesome/fontawesome-svg-core/styles.css",
                "node_modules/prism-themes/themes/prism-ghcolors.css",
                "src/styles/styles.scss",
                {
                  "inject": false,
                  "input": "src/styles/themes/theme-blackbeauty.scss",
                  "bundleName": "theme-blackbeauty"
                },
                {
                  "inject": false,
                  "input": "src/styles/themes/theme-westermann-blue.scss",
                  "bundleName": "theme-westermann-blue"
                },
                {
                  "inject": false,
                  "input": "src/styles/themes/theme-westermann-blue.scss",
                  "bundleName": "theme-default"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "frontend:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "frontend:build:production"
            },
            "thmStaging": {
              "browserTarget": "frontend:build:thmStaging"
            },
            "thmBeta": {
              "browserTarget": "frontend:build:thmBeta"
            },
            "westermann": {
              "browserTarget": "frontend:build:westermann"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "frontend:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "styles": [
              "src/styles/styles.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/styles/themes"
              ]
            },
            "scripts": [
              "node_modules/marked/lib/marked.js",
              "node_modules/prismjs/prism.js",
              "node_modules/prismjs/components.js"
            ],
            "assets": [
              "src/assets/fonts",
              "src/assets/js",
              "src/assets/i18n",
              "src/assets/images",
              "src/assets/styles",
              "src/assets/jobs",
              "src/assets/static",
              "src/assets/serverEndpoint.json",
              "src/assets/imageDerivates.json",
              "src/assets/themeData.json",
              {
                "glob": "click-service-worker.js",
                "input": "src",
                "output": "/"
              }
            ],
            "codeCoverageExclude": [
              "/**/*mock*.ts"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": []
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/frontend/server",
            "main": "src/server.ts",
            "tsConfig": "src/tsconfig.server.json",
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/styles/themes"
              ]
            }
          },
          "configurations": {
            "production": {
              "optimization": {
                "scripts": true,
                "styles": false
              },
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "sourceMap": false
            },
            "thmStaging": {
              "optimization": {
                "scripts": true,
                "styles": false
              },
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.thm.staging.ts"
                }
              ],
              "sourceMap": false
            },
            "thmBeta": {
              "optimization": {
                "scripts": true,
                "styles": false
              },
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.thm.beta.ts"
                }
              ],
              "sourceMap": false
            },
            "westermann": {
              "optimization": {
                "scripts": true,
                "styles": false
              },
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.westermann.ts"
                }
              ],
              "sourceMap": false
            }
          }
        },
        "serve-ssr": {
          "builder": "@nguniversal/builders:ssr-dev-server",
          "options": {
            "browserTarget": "frontend:build",
            "serverTarget": "frontend:server"
          },
          "configurations": {
            "production": {
              "browserTarget": "frontend:build:production",
              "serverTarget": "frontend:server:production"
            }
          }
        },
        "prerender": {
          "builder": "@nguniversal/builders:prerender",
          "options": {
            "browserTarget": "frontend:build",
            "serverTarget": "frontend:server",
            "routes": [
              "info/about",
              "info/tos",
              "info/imprint",
              "info/dataprivacy",
              "languages",
              "themes",
              "login",
              "/"
            ]
          },
          "configurations": {
            "production": {
              "browserTarget": "frontend:build:production",
              "serverTarget": "frontend:server:production"
            },
            "thmStaging": {
              "browserTarget": "frontend:build:thmStaging",
              "serverTarget": "frontend:server:thmStaging"
            },
            "thmBeta": {
              "browserTarget": "frontend:build:thmBeta",
              "serverTarget": "frontend:server:thmBeta"
            },
            "westermann": {
              "browserTarget": "frontend:build:westermann",
              "serverTarget": "frontend:server:westermann"
            }
          }
        }
      }
    }
  },
  "defaultProject": "frontend",
  "schematics": {
    "@schematics/angular:component": {
      "styleext": "scss"
    }
  },
  "cli": {
    "analytics": false
  }
}
